<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
      <stateVariable>
         <name>Watts</name>
         <sendEventsAttribute>yes</sendEventsAttribute>
         <dataType>i2</dataType>
        <shortCode>Watts</shortCode>
      </stateVariable>
      <stateVariable>
         <name>KWH</name>
         <sendEventsAttribute>yes</sendEventsAttribute>
         <dataType>i2</dataType>
        <shortCode>KWH</shortCode>
      </stateVariable>
      <stateVariable>
      	<name>Voltage</name>
      	<sendEventsAttribute>yes</sendEventsAttribute>
      	<dataType>i2</dataType>
	<shortCode>Voltage</shortCode>
      </stateVariable>
      <stateVariable>
      	<name>Current</name>
      	<sendEventsAttribute>yes</sendEventsAttribute>
      	<dataType>i2</dataType>
	<shortCode>Current</shortCode>
      </stateVariable>
  </serviceStateTable>
  <actionList>
  	<action>
  		<name>GetImpedance</name>
  		<argumentList>
  			<argument>
  				<name>ImpedanceValue</name>
  				<direction>out</direction>
				<relatedStateVariable>Impedance</relatedStateVariable>
			</argument>
		</argumentList>
	</action>
  	<action>
  		<name>GetVoltage</name>
  		<argumentList>
  			<argument>
  				<name>VoltageValue</name>
  				<direction>out</direction>
				<relatedStateVariable>Voltage</relatedStateVariable>
			</argument>
		</argumentList>
	</action>
  	<action>
  		<name>GetCurrent</name>
  		<argumentList>
  			<argument>
  				<name>CurrentValue</name>
  				<direction>out</direction>
				<relatedStateVariable>Current</relatedStateVariable>
			</argument>
		</argumentList>
	</action>
</actionList>
</scpd>
